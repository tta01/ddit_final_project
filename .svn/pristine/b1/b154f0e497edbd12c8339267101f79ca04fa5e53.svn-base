body{
	background-color:#fff;
}

.cen {
	width:700px;
}

.ent-review-header {
	display:flex;
	align-items:center;
	margin-bottom:40px;
}

.ent-review-header img{
	width:72px;
}

.ent-review-header span{
	font-size:14px;
	color:#686a6d;
	font-weight:bold;
	line-height:15px;
}

.input01 {
	width:100%;
	margin-bottom:45px
}


.name-label {
    font-size: 16px;
    font-weight:bold;
    margin-bottom: 14px;
}

.div-harf {
	display:flex;
	justify-content:space-between;
}

.div-harf > div {
	width:48.5%;
}

.sub-label{
	font-size:12px;
	color : rgb(164, 166, 173);
	margin-bottom: 6px;
}

.rate-area {
	display:grid;
	align-items:center;
	grid-template-columns: 200px auto;
}


fieldset {
	margin-bottom:40px;
}