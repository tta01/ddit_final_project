}

.label-switch {
	display: inline-flex;
	align-items: center;
	gap: 0.5rem;
	cursor: pointer;
}

input[role="switch"] {
	appearance: none;
	position: relative;
	border: max(2px, 0.1em) solid gray;
	border-radius: 1.25em;
	width: 2.25em;
	height: 1.25em;
}

input[role="switch"]::before {
	content: "";
	position: absolute;
	left: 0;
	width: 1em;
	height: 1em;
	border-radius: 50%;
	transform: scale(0.8);
	background-color: gray;
	transition: left 0.1s linear;
}

input[role="switch"]:checked::before {
	background-color: white;
	left: 1em;
}

input[role="switch"]:checked {
	background-color: #00c362;
	border-color: #00c362;
}

