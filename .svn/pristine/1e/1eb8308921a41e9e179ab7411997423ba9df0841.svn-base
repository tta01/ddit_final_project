* {
	margin: 0;
	padding: 0;
	box-sizing: border-box;
	font: inherit;
	color: inherit;
	color: #444;
}

html, body {
	height: 100%;
	font-family: "NotoSansKR-Regular";
}

a {
	text-decoration: none
}

table {
	border-collapse: collapse;
	border-spacing: 0
}

ul {
	list-style: none;
}

header {
	z-index: 999;
	background-color: #fff;
}

.strong {
	font-weight: bold;
}

.header-top {
	height: 50px;
	padding: 0 40px;
	display: flex;
	align-items: center;
	font-size: 15px;
}

.header-top-logo {
	display: flex;
	width: 112px;
	align-items: center;
	font-size: 20px;
	justify-content: center;
}

.header-top-logo img {
	width: 100%;
}

.alarm-box {
	display: inline-block;
	font-size: 20px;
	cursor: pointer;
}

.header-top-menu {
	margin-left: 50px;
	height: 100%;
	font-size: 14px;
}

.header-top-menu>ul {
	display: flex;
	align-items: center;
	height: 100%;
}

.header-top-menu>ul>li {
	margin: 0 16px;
	display: flex;
	align-items: center;
	height: 100%;
	position: relative;
}

.header-top a:hover {
	color: #00c362;
}

.header-top-menu>ul>li::before {
	content: "";
	position: absolute;
	width: 0;
	height: 2px;
	background-color: #00c362;
	bottom: 0;
	left: 50%;
	transform: translateX(-50%);
	transition: all 0.15s;
}

.header-top-menu ul li:hover::before {
	width: 100%;
}

.header-top-menu ul li.active::before {
	width: 100%;
}

.header-top-right {
	margin-left: auto;
	display: flex;
	gap: 5px;
	align-items: center;
	font-size: 14px;
}

.header-top-right>span {
	display: flex;
	align-items: center;
	justify-content: center;
}

.header-bot {
	min-height: 62px;
	border-top: 1px solid #e5e6e9;
	border-bottom: 1px solid #e5e6e9;
	display: flex;
	justify-content: center;
	align-items: center;
}

#sub {
	position: relative;
}

#sub ul {
	white-space: nowrap;
	text-align: left;
	display: none;
	position: absolute;
	top: 42px;
	left: 35px;
	transform: translateX(-50%);
	z-index: 999;
	background-color: #fff;
	border: 1px solid #d7dce5;
	border-radius: 4px;
}

#sub:hover>ul {
	display: block;
}

.sub-menu {
	border-left: 1px solid #fefefe;
	border-right: 1px solid #fefefe;
	border-bottom: 1px solid #fefefe;
}

.sub-menu>li {
	height: 52px;
	display: flex;
	align-items: center;
	justify-content: center;
	width: 100%;
	position: relative;
}

.sub-menu>li.active a {
	color: #00c362;
	font-weight: bold
}

.sub-menu>li>a {
	width: 100%;
	height: 100%;
	padding: 15px 35px;
	text-align: center;
}

.sub-menu>li>a:hover {
	background-color: #f4f6fa;
}

.schbar {
	border: 1px solid #00c362;
	border-radius: 5px;
	width: 1060px;
	height: 48px;
	display: flex;
	align-items: center;
	position: relative;
}

.schbar .schbar-input {
	padding: 0 42px 0 16px;
	width: 100%;
	height: 90%;
	border: none;
	outline: none;
}

.schbar::before {
	content: "\f002";
	display: inline-block;
	position: absolute;
	top: 50%;
	right: 0;
	transform: translateY(-50%);
	width: 18px;
	height: 18px;
	font-family: "Font Awesome 5 Free";
	font-weight: 900;
	margin-right: 18px;
	color: #00c362;
	cursor: pointer;
}

.schbar .schbar-input::placeholder {
	font-weight: bold;
	letter-spacing: 0.04em;
	color: #ccc;
}

#scroll-header {
	padding: 0 40px;
	display: none;
	position: fixed;
	top: 0;
	align-items: center;
	height: 62px;
	justify-content: center;
	width: 100%;
	border-top: 1px solid #e5e6e9;
	border-bottom: 1px solid #e5e6e9;
	z-index: 9999;
}

#scroll-header .header-top-logo {
	position: absolute;
	left: 40px;
	top: 50%;
	transform: translateY(-50%);
}

