.bb::before {
	content: "";
	position: absolute;
	top: calc(100% - 366px);
	left: 50%;
	z-index: 0;
	transform: translateX(-50%);
	width: 3500px;
	height: 1700px;
	border-radius: 100%;
	background: linear-gradient(0deg, rgba(255, 255, 255, 0) 76.74%,
		rgba(171, 205, 255, 0.3) 100%);
	pointer-events: none;
}

.event-card a {
	background-size: cover;
	background-position: center;
	background-repeat: no-repeat;
	border-radius: 10px;
	transition: all 0.2s;
}

.event-card:hover a {
	box-shadow: 0px 0px 12px -0px rgba(0, 0, 0, 0.6);
}

.event-card a {
	display: inline-block;
	width: 100%;
	height: 100%;
}

.event-card:nth-child(1) {
