<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jobja.member.mapper.MemberAuthMapper">
	
	<insert id="createPostAuth" parameterType="String">
		INSERT INTO AUTH (MEM_ID,ROLE)
					VALUES (#{memId},'ROLE_MEMBER')
	</insert>
	
	<insert id="createPostEntAuth" parameterType="String">
		INSERT INTO AUTH (MEM_ID,ROLE)
					VALUES (#{memId},'ROLE_MEMBER')
	</insert>


</mapper>